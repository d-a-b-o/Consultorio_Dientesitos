@model WebDientesitos.Models.Usuario
<head>
    <link rel="stylesheet" href="~/css/LoginStyle.css">
</head>
<body>
    <form asp-controller="Usuario" asp-action="MenuUsuario" method="post">
        <h1>Iniciar Sesión</h1>
        <div class="form-group">
            <label for="username">Dni:</label>
            <input type="text" id="username" name="dni" asp-for="dni" required>
        </div>
        <div class="form-group">
            <label for="password">Contraseña:</label>
            <input type="password" id="password" name="password"  asp-for="contrasena" required>
        </div>
        <button type="submit">Ingresar</button>
    </form>
</body>